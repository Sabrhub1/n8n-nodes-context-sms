{
  "name": "n8n-nodes-context-sms",
  "version": "1.1.1",
  "description": "n8n node for Context SMS integration",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "context-sms",
    "sabrhub",
    "sms",
    "messaging"
  ],
  "license": "MIT",
  "homepage": "https://www.npmjs.com/package/n8n-nodes-context-sms",
  "author": {
    "name": "Sabrhub",
    "email": "support@sabrhub.com"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist/**/*",
    "index.js",
    "README.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SabrhubApi/SabrhubApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Sabrhub/Sabrhub.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-svgmin": "^4.0.3",
    "n8n-workflow": "*",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
